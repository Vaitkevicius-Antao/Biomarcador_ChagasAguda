        "## Matriz de Confus√£o + M√©tricas ##\n",
        "\n",
        "def evaluate_model(x, y, model_name='Modelo', n_splits=5):\n",
        "    skf = StratifiedKFold(n_splits=n_splits, shuffle=True, random_state=42)\n",
        "\n",
        "    cm_total = np.array([[0,0], [0,0]])\n",
        "    acc_list = []\n",
        "    prec_list = []\n",
        "    recall_list = []\n",
        "    f1_list = []\n",
        "    auc_list = []\n",
        "\n",
        "    for train_index, test_index in skf.split(x, y):\n",
        "        x_train, x_test = x.iloc[train_index], x.iloc[test_index]\n",
        "        y_train, y_test = y.iloc[train_index], y.iloc[test_index]\n",
        "\n",
        "        model = RandomForestClassifier(random_state=42)\n",
        "        model.fit(x_train, y_train)\n",
        "\n",
        "        y_pred = model.predict(x_test)\n",
        "        y_proba = model.predict_proba(x_test)[:, 1] # Prob. classe 1 (INFECTADO)\n",
        "\n",
        "        cm = confusion_matrix(y_test, y_pred, labels=[0, 1])\n",
        "        cm_total += cm\n",
        "\n",
        "        acc_list.append(accuracy_score(y_test, y_pred))\n",
        "        prec_list.append(precision_score(y_test, y_pred, zero_division=0))\n",
        "        recall_list.append(recall_score(y_test, y_pred, zero_division=0))\n",
        "        f1_list.append(f1_score(y_test, y_pred, zero_division=0))\n",
        "        auc_list.append(roc_auc_score(y_test, y_proba))\n",
        "\n",
        "    plt.figure(figsize=(5, 4))\n",
        "    sns.heatmap(cm_total, annot=True, fmt='d', cmap='Blues', cbar=False,\n",
        "                xticklabels=['N-INF', 'INF'], yticklabels=['N-INF', 'INF'])\n",
        "    plt.xlabel('Predito')\n",
        "    plt.ylabel('Real')\n",
        "    plt.title(f'Matriz de Confus√£o - {model_name}')\n",
        "    plt.show()\n",
        "\n",
        "\n",
        "    results = pd.DataFrame({\n",
        "        'Modelo': [model_name],\n",
        "        'Acuracia': [np.mean(acc_list)],\n",
        "        'Precisao': [np.mean(prec_list)],\n",
        "        'Recall': [np.mean(recall_list)],\n",
        "        'F1': [np.mean(f1_list)],\n",
        "        'AUC': [np.mean(auc_list)]\n",
        "    })\n",
        "\n",
        "    return results\n",
        "\n",
        "resultado_mac = evaluate_model(df[mac_features], y, model_name = 'MACs')\n",
        "resultado_parasit = evaluate_model(df[['PARASIT']], y, model_name='Parasitol√≥gico')\n",
        "resultado_IgM = evaluate_model(df[['IgM']], y, model_name='Sorol√≥gico IgM')\n",
        "resultado_mol = evaluate_model(df[['MOL']], y, model_name='qPCR')\n",
        "resultado_macExame = evaluate_model(df[mac_features + exames_features], y, model_name='MACs + Exames')\n",
        "resultado_vcam = evaluate_model(df[['CD106']], y, model_name='VCAM')\n",
        "resultado_vcam_Lselec = evaluate_model(df[['CD106','CD62L']], y, model_name='VCAM + Lselec')\n",
        "resultado_vcam_IgM = evaluate_model(df[['CD106','IgM']], y, model_name='VCAM + IgM')\n",
        "resultado_mac_IgM = evaluate_model(df[['CD106','CD62P','CD62L','CD62E','IgM']], y, model_name='MACs + IgM')\n",
        "\n",
        "resultados = pd.concat([resultado_mac, resultado_parasit, resultado_IgM, resultado_mol,\n",
        "                        resultado_macExame, resultado_vcam, resultado_vcam_Lselec,\n",
        "                        resultado_vcam_IgM, resultado_mac_IgM])\n",
        "print(resultados)"
      ],
